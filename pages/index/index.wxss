.works{padding: 90rpx 0 70rpx; background: #f0f3f8;}
.wkMain{display: flex; flex-wrap: wrap;justify-content: space-between;}
.wkMain .box{width: 344rpx; height: 214rpx;margin-bottom: 10rpx; position: relative;}
.wkMain .pic{width: 100%; height: 100%;  }
.wkMain .ceng{position: absolute; top: 0; left: 0; align-content: center; width: 100%;height: 100%; background: rgba(0,0,0,0.5);color: #fff;display: flex; flex-direction: column; justify-content: center; align-items: center; padding: 0 20rpx; box-sizing: border-box; text-align: center;}
.wkMain .tit{font-size:34rpx; }
.wkMain .line{width: 70rpx;height: 2rpx;background: #fff; margin: 10rpx 0 20rpx;}
.wkMain .des{font-size:24rpx; opacity: 0.6;text-transform: uppercase; letter-spacing: 5rpx;}

.dynamic{padding: 90rpx 0 70rpx;}
.dyItem{display: flex;  padding: 15rpx 0;border: 1rpx dashed #f4f4f4; justify-content: space-between;}
.dyItem .pic{width: 230rpx; height: 150rpx;}
.dyItem .pic image{width: 100%; height: 100%}
.dyItem .txt{width: 400rpx; display: flex; flex-direction: column; justify-content: space-between;}
.dyItem .title{font-size: 34rpx;}
.dyItem .info{font-size: 26rpx; color: #888;}
.info text{font-size: 22rpx;}




